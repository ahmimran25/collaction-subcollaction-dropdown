<collection-grid-{{ ai_gen_id }}
  class="ai-collection-grid-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  {% for i in (1..8) %}
    {% liquid
      assign collection_key = 'main_collection_' | append: i
      assign main_collection = block.settings[collection_key]
      
      assign sub1_key = 'sub_collection_' | append: i | append: '_1'
      assign sub2_key = 'sub_collection_' | append: i | append: '_2'
      assign sub3_key = 'sub_collection_' | append: i | append: '_3'
      assign sub4_key = 'sub_collection_' | append: i | append: '_4'
      
      assign sub_collection_1 = block.settings[sub1_key]
      assign sub_collection_2 = block.settings[sub2_key]
      assign sub_collection_3 = block.settings[sub3_key]
      assign sub_collection_4 = block.settings[sub4_key]
    %}

    {% if main_collection != blank %}
      <div class="ai-collection-card-{{ ai_gen_id }}" data-collection-id="{{ i }}">
        <div class="ai-collection-image-wrapper-{{ ai_gen_id }}">
          {% if main_collection.featured_image %}
            <img
              src="{{ main_collection.featured_image | image_url: width: 600 }}"
              alt="{{ main_collection.featured_image.alt | escape }}"
              class="ai-collection-image-{{ ai_gen_id }}"
              loading="lazy"
              width="600"
              height="{{ block.settings.image_height }}"
            >
          {% else %}
            <div class="ai-collection-image-placeholder-{{ ai_gen_id }}">
              {{ 'collection-1' | placeholder_svg_tag }}
            </div>
          {% endif %}
        </div>

        <div class="ai-collection-content-{{ ai_gen_id }}">
          <a href="{{ main_collection.url }}" class="ai-collection-title-{{ ai_gen_id }}">
            {{ main_collection.title }}
          </a>

          {% if main_collection.description != blank and block.settings.show_description %}
            <div class="ai-collection-description-{{ ai_gen_id }}">
              {{ main_collection.description | truncate: 120 }}
            </div>
          {% endif %}

          <div class="ai-collection-actions-{{ ai_gen_id }}">
            <a href="{{ main_collection.url }}" class="ai-collection-view-btn-{{ ai_gen_id }}">
              {{ block.settings.view_collection_text }}
            </a>

            {% if sub_collection_1 != blank or sub_collection_2 != blank or sub_collection_3 != blank or sub_collection_4 != blank %}
              <button
                class="ai-collection-expand-btn-{{ ai_gen_id }}"
                data-expand-target="subcollections-{{ ai_gen_id }}-{{ i }}"
                aria-expanded="false"
              >
                {{ block.settings.view_subcollections_text }}
                <svg class="ai-collection-expand-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                  <polyline points="6,9 12,15 18,9"></polyline>
                </svg>
              </button>
            {% endif %}
          </div>
        </div>

        {% if sub_collection_1 != blank or sub_collection_2 != blank or sub_collection_3 != blank or sub_collection_4 != blank %}
          <div
            class="ai-subcollections-{{ ai_gen_id }}"
            id="subcollections-{{ ai_gen_id }}-{{ i }}"
          >
            <div class="ai-subcollections-list-{{ ai_gen_id }}">
              {% if sub_collection_1 != blank %}
                <div class="ai-subcollection-item-{{ ai_gen_id }}">
                  <a href="{{ sub_collection_1.url }}" class="ai-subcollection-link-{{ ai_gen_id }}">
                    {% if sub_collection_1.featured_image %}
                      <img
                        src="{{ sub_collection_1.featured_image | image_url: width: 160 }}"
                        alt="{{ sub_collection_1.featured_image.alt | escape }}"
                        class="ai-subcollection-image-{{ ai_gen_id }}"
                        loading="lazy"
                        width="80"
                        height="80"
                      >
                    {% else %}
                      <div class="ai-subcollection-image-placeholder-{{ ai_gen_id }}">
                        {{ 'collection-1' | placeholder_svg_tag }}
                      </div>
                    {% endif %}
                    <h4 class="ai-subcollection-title-{{ ai_gen_id }}">{{ sub_collection_1.title }}</h4>
                  </a>
                </div>
              {% endif %}

              {% if sub_collection_2 != blank %}
                <div class="ai-subcollection-item-{{ ai_gen_id }}">
                  <a href="{{ sub_collection_2.url }}" class="ai-subcollection-link-{{ ai_gen_id }}">
                    {% if sub_collection_2.featured_image %}
                      <img
                        src="{{ sub_collection_2.featured_image | image_url: width: 160 }}"
                        alt="{{ sub_collection_2.featured_image.alt | escape }}"
                        class="ai-subcollection-image-{{ ai_gen_id }}"
                        loading="lazy"
                        width="80"
                        height="80"
                      >
                    {% else %}
                      <div class="ai-subcollection-image-placeholder-{{ ai_gen_id }}">
                        {{ 'collection-1' | placeholder_svg_tag }}
                      </div>
                    {% endif %}
                    <h4 class="ai-subcollection-title-{{ ai_gen_id }}">{{ sub_collection_2.title }}</h4>
                  </a>
                </div>
              {% endif %}

              {% if sub_collection_3 != blank %}
                <div class="ai-subcollection-item-{{ ai_gen_id }}">
                  <a href="{{ sub_collection_3.url }}" class="ai-subcollection-link-{{ ai_gen_id }}">
                    {% if sub_collection_3.featured_image %}
                      <img
                        src="{{ sub_collection_3.featured_image | image_url: width: 160 }}"
                        alt="{{ sub_collection_3.featured_image.alt | escape }}"
                        class="ai-subcollection-image-{{ ai_gen_id }}"
                        loading="lazy"
                        width="80"
                        height="80"
                      >
                    {% else %}
                      <div class="ai-subcollection-image-placeholder-{{ ai_gen_id }}">
                        {{ 'collection-1' | placeholder_svg_tag }}
                      </div>
                    {% endif %}
                    <h4 class="ai-subcollection-title-{{ ai_gen_id }}">{{ sub_collection_3.title }}</h4>
                  </a>
                </div>
              {% endif %}

              {% if sub_collection_4 != blank %}
                <div class="ai-subcollection-item-{{ ai_gen_id }}">
                  <a href="{{ sub_collection_4.url }}" class="ai-subcollection-link-{{ ai_gen_id }}">
                    {% if sub_collection_4.featured_image %}
                      <img
                        src="{{ sub_collection_4.featured_image | image_url: width: 160 }}"
                        alt="{{ sub_collection_4.featured_image.alt | escape }}"
                        class="ai-subcollection-image-{{ ai_gen_id }}"
                        loading="lazy"
                        width="80"
                        height="80"
                      >
                    {% else %}
                      <div class="ai-subcollection-image-placeholder-{{ ai_gen_id }}">
                        {{ 'collection-1' | placeholder_svg_tag }}
                      </div>
                    {% endif %}
                    <h4 class="ai-subcollection-title-{{ ai_gen_id }}">{{ sub_collection_4.title }}</h4>
                  </a>
                </div>
              {% endif %}
            </div>
          </div>
        {% endif %}
      </div>
    {% endif %}
  {% endfor %}
</collection-grid-{{ ai_gen_id }}>

<script>
  (function() {
    class CollectionGrid{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.expandButtons = this.querySelectorAll('.ai-collection-expand-btn-{{ ai_gen_id }}');
        this.setupEventListeners();
      }

      setupEventListeners() {
        this.expandButtons.forEach((button) => {
          button.addEventListener('click', (event) => {
            event.preventDefault();
            this.toggleSubcollections(button);
          });
        });
      }

      toggleSubcollections(button) {
        const targetId = button.getAttribute('data-expand-target');
        const subcollectionsPanel = document.getElementById(targetId);
        const isExpanded = button.classList.contains('expanded');

        if (isExpanded) {
          button.classList.remove('expanded');
          subcollectionsPanel.classList.remove('expanded');
          button.setAttribute('aria-expanded', 'false');
        } else {
          this.closeAllPanels();
          button.classList.add('expanded');
          subcollectionsPanel.classList.add('expanded');
          button.setAttribute('aria-expanded', 'true');
        }
      }

      closeAllPanels() {
        this.expandButtons.forEach((btn) => {
          btn.classList.remove('expanded');
          btn.setAttribute('aria-expanded', 'false');
        });

        this.querySelectorAll('.ai-subcollections-{{ ai_gen_id }}').forEach((panel) => {
          panel.classList.remove('expanded');
        });
      }
    }

    customElements.define('collection-grid-{{ ai_gen_id }}', CollectionGrid{{ ai_gen_id }});
  })();
</script>

