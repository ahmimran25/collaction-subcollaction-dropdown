{% doc %}
  @prompt
    Create a responsive collection list grid main collection should appear in a grid card layout. Under each main collection, include an option/button to add multiple sub-collections.
    
    When a user clicks on a main collection, a dropdown or expandable section should open below it showing all the sub-collections belonging to that main collection., add grid option 
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-collection-grid-{{ ai_gen_id }} {
    display: grid;
    {% if block.settings.grid_layout == 'auto' %}
      grid-template-columns: repeat(auto-fit, minmax({{ block.settings.min_card_width }}px, 1fr));
    {% else %}
      grid-template-columns: repeat({{ block.settings.columns_desktop }}, 1fr);
    {% endif %}
    gap: {{ block.settings.grid_gap }}px;
    padding: {{ block.settings.section_padding }}px 0;
  }

  .ai-collection-card-{{ ai_gen_id }} {
    position: relative;
    background-color: {{ block.settings.card_background_color }};
    border-radius: {{ block.settings.card_border_radius }}px;
    overflow: hidden;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    border: {{ block.settings.card_border_width }}px solid {{ block.settings.card_border_color }};
  }

  .ai-collection-card-{{ ai_gen_id }}:hover {
    transform: translateY(-{{ block.settings.hover_lift }}px);
    box-shadow: 0 {{ block.settings.hover_lift | times: 2 }}px {{ block.settings.hover_lift | times: 4 }}px rgba(0, 0, 0, 0.1);
  }

  .ai-collection-image-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    height: {{ block.settings.image_height }}px;
    overflow: hidden;
  }

  .ai-collection-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
  }

  .ai-collection-card-{{ ai_gen_id }}:hover .ai-collection-image-{{ ai_gen_id }} {
    transform: scale(1.05);
  }

  .ai-collection-image-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-collection-image-placeholder-{{ ai_gen_id }} svg {
    width: 60px;
    height: 60px;
    opacity: 0.5;
  }

  .ai-collection-content-{{ ai_gen_id }} {
    padding: {{ block.settings.card_padding }}px;
  }

  .ai-collection-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_font_size }}px;
    font-weight: 600;
    color: {{ block.settings.title_color }};
    margin: 0 0 8px;
    text-decoration: none;
    display: block;
  }

  .ai-collection-title-{{ ai_gen_id }}:hover {
    color: {{ block.settings.title_hover_color }};
  }

  .ai-collection-description-{{ ai_gen_id }} {
    font-size: {{ block.settings.description_font_size }}px;
    color: {{ block.settings.description_color }};
    margin: 0 0 16px;
    line-height: 1.4;
  }

  .ai-collection-actions-{{ ai_gen_id }} {
    display: flex;
    gap: 8px;
    align-items: center;
  }

  .ai-collection-view-btn-{{ ai_gen_id }} {
    padding: 8px 16px;
    background-color: {{ block.settings.primary_button_color }};
    color: {{ block.settings.primary_button_text_color }};
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    text-decoration: none;
    font-size: 14px;
    font-weight: 500;
    transition: background-color 0.3s ease;
    display: inline-block;
  }

  .ai-collection-view-btn-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.primary_button_hover_color }};
    color: {{ block.settings.primary_button_text_color }};
  }

  .ai-collection-expand-btn-{{ ai_gen_id }} {
    padding: 8px 12px;
    background-color: {{ block.settings.secondary_button_color }};
    color: {{ block.settings.secondary_button_text_color }};
    border: 1px solid {{ block.settings.secondary_button_border_color }};
    border-radius: {{ block.settings.button_border_radius }}px;
    cursor: pointer;
    font-size: 14px;
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
    gap: 4px;
  }

  .ai-collection-expand-btn-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.secondary_button_hover_color }};
    color: {{ block.settings.secondary_button_hover_text_color }};
    border-color: {{ block.settings.secondary_button_hover_border_color }};
  }

  .ai-collection-expand-icon-{{ ai_gen_id }} {
    width: 16px;
    height: 16px;
    transition: transform 0.3s ease;
  }

  .ai-collection-expand-btn-{{ ai_gen_id }}.expanded .ai-collection-expand-icon-{{ ai_gen_id }} {
    transform: rotate(180deg);
  }

  .ai-subcollections-{{ ai_gen_id }} {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.3s ease, padding 0.3s ease;
    background-color: {{ block.settings.subcollection_background_color }};
    border-top: 1px solid {{ block.settings.card_border_color }};
  }

  .ai-subcollections-{{ ai_gen_id }}.expanded {
    max-height: 500px;
    padding: {{ block.settings.card_padding }}px;
  }

  .ai-subcollections-list-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(120px, 1fr));
    gap: 12px;
  }

  .ai-subcollection-item-{{ ai_gen_id }} {
    text-align: center;
  }

  .ai-subcollection-link-{{ ai_gen_id }} {
    display: block;
    text-decoration: none;
    color: {{ block.settings.subcollection_text_color }};
    transition: color 0.3s ease;
  }

  .ai-subcollection-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.subcollection_hover_color }};
  }

  .ai-subcollection-image-{{ ai_gen_id }} {
    width: 80px;
    height: 80px;
    border-radius: 50%;
    object-fit: cover;
    margin: 0 auto 8px;
    display: block;
  }

  .ai-subcollection-image-placeholder-{{ ai_gen_id }} {
    width: 80px;
    height: 80px;
    border-radius: 50%;
    background-color: #f4f4f4;
    margin: 0 auto 8px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-subcollection-image-placeholder-{{ ai_gen_id }} svg {
    width: 30px;
    height: 30px;
    opacity: 0.5;
  }

  .ai-subcollection-title-{{ ai_gen_id }} {
    font-size: 12px;
    font-weight: 500;
    margin: 0;
  }

  .ai-empty-subcollections-{{ ai_gen_id }} {
    text-align: center;
    color: {{ block.settings.description_color }};
    font-style: italic;
    padding: 20px;
  }
